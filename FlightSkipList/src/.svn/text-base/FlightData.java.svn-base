public class FlightData {

	private String flightNumber;
	private int price;

	public FlightData() {
		flightNumber = null;
		price = 0;
	}

	public FlightData(String flightNum, int p) {
		flightNumber = flightNum;
		price = p;
	}

	@Override
	public String toString() {
		return flightNumber + " " + price;
	}

}
